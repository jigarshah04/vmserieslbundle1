{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminUsername": {
      "value": "<YourUserName>"
    },
    "adminPassword": {
      "value": "<YourPassword>"
    },
    "newStorageAccount": {
      "value": "<UniqueLowerCaseStorageAccount-Will Be Created By Template>"
    },
    "dnsNameForPublicIP": {
      "value": "<UniqueDNSName-WillBeCreatedByTemplate>"
    },
    "vmName": {
      "value": "VM-Series 7.1.0"
    },
    "vmSize": {
      "value": "Standard_D3"
    },
    "srcIPInboundNSG": {
      "value": "<YourSourceIP-Sets the inbound Default NSG>"
    },
    "ipAddressPrefix": {
      "value": "192.168"
    }
  }
}